extends _layouts/home

block styles
	link(rel="stylesheet", href="css/owl.carousel.min.css")
	link(rel="stylesheet", href="css/owl.theme.default.min.css")
	link(rel="stylesheet", href="css/contacts.css")
	link(rel="stylesheet", href="css/schedules.css")


block content

	include _partials/header.pug

	main
		.container
			section.header-breadcrumbs
				.breadcrumbs #[a.home(href="#") Главная] #[span.slash /] #[a.active(href="schedules.html") Расписание]
			section.header
				.row
					.col-12.col-xl-4
						h1 Расписание
					.col-12.col-xl-8
						.header-desc
							.line
							p.text Отметьте тренировки из расписания, на которые вы хотите записаться и нажмите "Записаться на занятия". Количество тренировок должно подходить под один из доступных абонементов:
							.variations.d-flex
								.varCol
									span на 1 занятие - 900 р.
									span на 4 занятия - 3 000 р.
								.varCol
									span на 8 занятий - 5 200 р.
									span на 12 занятий - 6 800 р.
			section.calendar
				.owl-carousel.carousel#schedulesCsl
					.slide
						.topLine
							.buttonPrev.d-flex.align-items-center.justify-content-center
								img(src="img/arrow-right_black.png", alt="arrow-right_black.png")
							.buttonNext.d-flex.align-items-center.justify-content-center
								img(src="img/arrow-right_black.png", alt="arrow-right_black.png")
							span 24 июня-30 июня
						.tableWeek
