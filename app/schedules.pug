									th.date.gray(colspan="1")
										span 25 июня, ВТ
									th.date(colspan="1")
										span 26 июня, СР
									th.date.gray(colspan="1")
										span 27 июня, ЧТ
									th.date(colspan="1")
										span 28 июня, ПТ
									th.date.gray(colspan="1")
										span 29 июня, СБ
									th.date(colspan="1")
										span 30 июня, ВС
								tr
									td
										.class
											.class-head.d-flex.align-items-center
												.text Ягодицы+#[wbr]Ноги+Пресс
											.separLine
											.elems
												.elem.d-flex
													img.icon(src="img/schedules/clock.png", alt="clock.png")
